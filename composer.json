{
	"name": "laravel/laravel",
	"type": "project",
	"description": "The Laravel Framework.",
    "keywords": [
		"framework",
		"laravel"
	],
	"license": "MIT",
	"require": {
		"php": "^8.0.2",
		"darkaonline/l5-swagger": "^8.4",
		"guzzlehttp/guzzle": "^7.2",
		"laravel-json-api/laravel": "^2.5",
		"laravel-json-api/validation": "^2.1",
		"laravel/framework": "^9.19",
		"laravel/passport": "^11.2",
		"laravel/sanctum": "^3.0",
		"laravel/tinker": "^2.7"
	},
	"require-dev": {
		"barryvdh/laravel-debugbar": "^3.7",
		"fakerphp/faker": "^1.9.1",
		"laravel-json-api/testing": "^1.1",
		"laravel/pint": "^1.0",
		"laravel/sail": "^1.16",
		"mockery/mockery": "^1.4.4",
		"nunomaduro/collision": "^6.1",
		"nunomaduro/larastan": "^2.0",
		"pestphp/pest": "^1.22",
		"pestphp/pest-plugin-laravel": "^1.3",
		"phpcompatibility/php-compatibility": "*",
		"phpunit/phpunit": "^9.5.10",
		"spatie/laravel-ignition": "^1.0",
		"squizlabs/php_codesniffer": "^3.7"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app/",
			"Database\\Factories\\": "database/factories/",
			"Database\\Seeders\\": "database/seeders/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"scripts": {
		"post-autoload-dump": [
			"Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
			"@php artisan package:discover --ansi"
		],
		"post-install-cmd": [
			"\"vendor/bin/phpcs\" --config-set installed_paths vendor/phpcompatibility/php-compatibility"
		],
		"post-update-cmd": [
			"@php artisan vendor:publish --tag=laravel-assets --ansi --force",
			"\"vendor/bin/phpcs\" --config-set installed_paths vendor/phpcompatibility/php-compatibility"
		],
		"post-root-package-install": [
			"@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
		],
		"post-create-project-cmd": [
			"@php artisan key:generate --ansi"
		],
		"test:types": "@php ./vendor/bin/phpstan analyse --ansi",
		"test:unit": "@php ./vendor/bin/pest --colors=always --exclude-group=integration",
		"test:integration": "@php ./vendor/bin/pest --colors=always --group=integration",
		"standards:list": "@php ./vendor/squizlabs/php_codesniffer/bin/phpcs -i",
		"standards:check": "@php ./vendor/squizlabs/php_codesniffer/bin/phpcs",
		"standards:fix": "@php ./vendor/squizlabs/php_codesniffer/bin/phpcbf"
	},
	"extra": {
		"laravel": {
			"dont-discover": []
		}
	},
	"config": {
		"optimize-autoloader": true,
		"preferred-install": "dist",
		"sort-packages": true,
		"allow-plugins": {
			"pestphp/pest-plugin": true
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
